<%= simple_form_for(@adminstrator) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :employee_id %>
    <%= f.input :lastname %>
    <%= f.input :firstname %>
    <%= f.input :email %>
    <%= f.input :auth_level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
